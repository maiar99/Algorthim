package projectalgo3;
import static projectalgo3.JobScheduler.scheduleJobs;

public class ProjectALGO3 {

    public static void main0(String[] args) {
        Job[] jobs = new Job[]{
                new Job(1, 6, 6),
                new Job(2, 5, 5),
                new Job(5, 7, 5),
                new Job(6, 8, 3)
        };
     
     
        int maxProfit = scheduleJobs(jobs);
        System.out.println("Maximum profit: " + maxProfit);    }
    
}
